{
  "name": "tshla-medical",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "./start-dev.sh",
    "dev:stop": "./stop-dev.sh",
    "build": "vite build",
    "build-with-types": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "quality": "npm run typecheck && npm run lint && npm run format:check",
    "preview": "vite preview",
    "db:migrate": "node src/lib/db/migrate.ts",
    "db:setup": "npm run db:migrate",
    "server:start": "node server/unified-api-gateway.js",
    "api:start": "PORT=3001 node server/minimal-api.js",
    "server:dev": "nodemon server/unified-api-gateway.js",
    "server:unified": "node server/unified-api-gateway.js",
    "server:pm2": "pm2 start server/unified-api-gateway.js --name tshla-unified-api --watch",
    "server:legacy:enhanced": "node server/enhanced-schedule-notes-api.js",
    "server:legacy:pump": "node server/pump-report-api.js",
    "server:pm2:stop": "pm2 stop tshla-api",
    "server:pm2:logs": "pm2 logs tshla-api",
    "server:pm2:restart": "pm2 restart tshla-api",
    "server:pm2:delete": "pm2 delete tshla-api",
    "server:health": "node server/healthcheck.js",
    "server:production": "pm2 start ecosystem.config.js --env production",
    "db:seed:pump": "node server/scripts/seed-pump-users.js",
    "validate:build": "./scripts/validate-build.sh",
    "validate:deployment": "./scripts/validate-deployment.sh",
    "validate:apis": "./scripts/validate-api-health.sh",
    "validate:db:prod": "node scripts/validate-db-prod.js",
    "validate:cors": "./scripts/validate-api-cors.sh",
    "validate:all": "npm run validate:build && npm run validate:db:prod && npm run validate:apis && npm run validate:cors && npm run validate:deployment",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.899.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@aws-sdk/client-transcribe-streaming": "^3.899.0",
    "@deepgram/sdk": "^4.11.2",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@stripe/react-stripe-js": "^4.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jspdf": "^1.3.3",
    "@types/jszip": "^3.4.0",
    "@types/qrcode": "^1.5.5",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.12.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "buffer": "^6.0.3",
    "dotenv": "^17.2.1",
    "html2canvas": "^1.4.1",
    "jose": "^5.2.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.539.0",
    "mysql2": "^3.15.0",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "nodemon": "^3.0.2",
    "openai": "^6.1.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pm2": "^5.3.0",
    "process": "^0.11.10",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.28.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "stream-browserify": "^3.0.0",
    "stripe": "^18.5.0",
    "util": "^0.12.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
